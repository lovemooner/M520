<!--index.wxml-->
<view class="container">
  <view class="userinfo">
    <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo" bindgetuserinfo="getUserInfo"> 获取头像昵称 </button>
    <block wx:else>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
  <view class="home-ip">
    <text class="home-ip-label">home-ip:</text><text class="home-ip">{{home_ip}}</text> 
  </view>
  <view>
     <text class="home-ip-label">updated-Date:</text><text class="home-ip">{{ip_updated}}</text>    </view>
    
  
  

<view class="btn-area">

 
  
</view>
 
</view>
